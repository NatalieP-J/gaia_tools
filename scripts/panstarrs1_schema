SET default_transaction_read_only= false;
CREATE TABLE gaiadr2_panstarrs1_best_neighbour (
    source_id bigint,
    original_ext_source_id bigint,
    angular_distance double precision,
    number_of_neighbours int,
    number_of_mates smallint,
    best_neighbour_multiplicity smallint,
    gaia_astrometric_params smallint,
    obj_name varchar,
    obj_id bigint,
    ra real,
    dec real,
    epoch_mean real,
    g_mean_psf_mag double precision,
    g_mean_psf_mag_error double precision,
    g_flags int,
    r_mean_psf_mag double precision,
    r_mean_psf_mag_error double precision,
    r_flags int,
    i_mean_psf_mag double precision,
    i_mean_psf_mag_error double precision,
    i_flags int,
    z_mean_psf_mag double precision,
    z_mean_psf_mag_error double precision,
    z_flags int,
    y_mean_psf_mag double precision,
    y_mean_psf_mag_error double precision,
    y_flags int,
    n_detections smallint,
    obj_info_flag int,
    quality_flag smallint
) WITHOUT OIDS;
